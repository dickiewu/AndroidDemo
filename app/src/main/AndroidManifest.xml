<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android = "http://schemas.android.com/apk/res/android"
          package = "name.dickie.android.demo">

    <uses-permission android:name = "android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name = "android.permission.SET_WALLPAPER" />
    <uses-permission android:name = "android.permission.INTERNET" />

    <application
        android:name = ".DemoApplication"
        android:allowBackup = "true"
        android:icon = "@mipmap/ic_launcher"
        android:label = "@string/app_name"
        android:roundIcon = "@mipmap/ic_launcher_round"
        android:supportsRtl = "true"
        android:theme = "@style/AppTheme">
        <activity
            android:name = ".MainActivity"
            android:icon = "@drawable/liveness_head_left"
            >
            <intent-filter>
                <action android:name = "android.intent.action.MAIN" />
                <category android:name = "android.intent.category.LAUNCHER" />
            </intent-filter>
            <intent-filter>
                <action android:name = "android.intent.action.VIEW" />

                <category android:name = "android.intent.category.BROWSABLE" />
                <!-- 作为activity 打开必须要有DEFAULT -->
                <category android:name = "android.intent.category.DEFAULT" />

                <data
                    android:host = "name.dickiewu.home"
                    android:scheme = "raven" />
            </intent-filter>
        </activity>
        <activity android:name = ".activity.TouchTestActivity">
            <intent-filter>
                <action android:name = "name.dickiewu.activity1" />

                <category android:name = "android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity android:name = ".activity.AActivity">
            <intent-filter>
                <action android:name = "android.intent.action.MAIN" />
                <category android:name = "android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name = ".activity.BActivity">
        </activity>
        <activity android:name = ".activity.CActivity">
        </activity>
    </application>

</manifest>